<tr>
    <td><strong><%= link_to category.abbv, edit_category_path(category.abbv), class: "link-dark link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover" %></strong></td>
    <td><%= category.cat_name %></td>
    <td><%= category.created_at %></td>
    <td><%= category.updated_at %></td>
    <td><%= button_to "Delete", category, method: :delete, class: 'btn btn-outline-success', onclick: "return confirm('Are you sure you want to delete this category?');" %></td>
</tr>