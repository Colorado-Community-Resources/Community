<tr>
    <td><strong><%= link_to question.questionID, edit_administrator_question_path(question.questionID), class: "link-dark link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover" %></strong></td>
    <td><%= question.question %></td>
    <td><%= question.admin_username %></td>
    <td><%= question.created_at %></td>
    <td><%= question.updated_at %></td>
    <td><%= button_to "Delete", administrator_question_path(question), method: :delete, class: 'btn btn-outline-success', onclick: "return confirm('Are you sure you want to delete this question?');" %></td>
</tr>