<tr>
  <td><strong><%= link_to review.reviewID, edit_administrator_review_path(review.reviewID), class: "link-dark link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover" %></strong></td>
    <td><%= review.organization&.org_name %></td>
    <td><%= review.userID_id %></td>
    <td><%= review.title %></td>
    <td><%= review.comment %></td>
    <td><%= review.starValue %></td>
    <td><%= review.reviewStatus ? "Approved" : "Pending" %></td>
    <td><%= review.adminID_id || "N/A" %></td>
    <td><%= review.created_at %></td>
    <td><%= review.updated_at %></td>
    <td><%= button_to "Delete", administrator_review_path(review), method: :delete, class: 'btn btn-outline-success', onclick: "return confirm('Are you sure you want to delete this review?');" %></td>
</tr>